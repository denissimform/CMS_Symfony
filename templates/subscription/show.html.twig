{% extends 'base.html.twig' %}

{% block title %}Subscription{% endblock %}

{% block body %}
    <h1>Subscription</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ subscription.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ subscription.type }}</td>
            </tr>
            <tr>
                <th>Criteria_dept</th>
                <td>{{ subscription.criteriaDept }}</td>
            </tr>
            <tr>
                <th>Criteria_user</th>
                <td>{{ subscription.criteriaUser }}</td>
            </tr>
            <tr>
                <th>Criteria_storage</th>
                <td>{{ subscription.criteriaStorage }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ subscription.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ subscription.createdAt ? subscription.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ subscription.updatedAt ? subscription.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_subscription_index') }}">back to list</a>

    <a href="{{ path('app_subscription_edit', {'id': subscription.id}) }}">edit</a>

    {{ include('subscription/_delete_form.html.twig') }}
{% endblock %}
