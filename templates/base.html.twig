<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
        <link rel="icon" href="{{ asset('favicon.png') }}">
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
	</head>
	<body>
		<!--  Body Wrapper -->
		<div
			class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full" data-sidebar-position="fixed" data-header-position="fixed">
			<!--  Sidebar Wrapper -->
			{% block sidebar %}{% endblock %}
			<!--  Sidebar End -->

			<!--  Main wrapper -->
			<div
				class="body-wrapper">
				<!--  Navbar Start -->
				{% block navbar %}{% endblock %}
				<!--  Navbar End -->

				<div
					class="container-fluid">
					<!-- Flash messages Start -->
					{% block flash %}{% endblock %}
					<!-- Flash messages end -->

					<!-- Layout Start -->
					{% block layout %}{% endblock %}
					<!-- Layout end -->

					<!-- Footer Start -->
					{% block footer %}{% endblock %}					
					<!-- Footer end -->
				</div>
			</div>
			<!--  Main End -->
		</div>
		{% block body %}{% endblock %}
		<!--  Body End -->

		<!-- Javascript start -->
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
		<!-- Javascript end -->
	</body>
</html>
